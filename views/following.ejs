<h1><%= username %> is Following</h1>
<hr>
<table>
	<% for(var i = follower.length-1; i >= 0; i--){
		if(follower[i].following == username){ %>
		<tr>
		<td width="60px"><img src="/images/bird.png"></td>
		<td style="padding-right:15px;">
			<a href="/users/<%= follower[i].follower %>"><%= follower[i].follower %></a>
		</td>
		</tr>
		<% }} %>
	</div>
