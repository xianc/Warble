<myUploads><a class="load">
	<i><font size="2">
		<% var k = 0;
		for(var i = warble.length-1; i >= 0; i--){
          	if(warble[i].username == username){
          		if(warble[i].attachment!='none' && warble[i].attachment!='') {
          			k = k + 1;
          		}
          	}
        } %>
        <%= k %>
	</font></i>
</a></myUploads>